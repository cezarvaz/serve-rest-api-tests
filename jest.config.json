{
  "verbose": true,
  "testTimeout": 70000,
  "moduleDirectories": [
    "node_modules",
    "gestao-performance-evaluation-api-tests"
  ],
  "setupFilesAfterEnv": ["jest-extended/all", "./utils/jestMatchers.js"],
  "reporters": [
    "default",
    [
      "jest-html-reporters",
      {
        "publicPath": "./html-report",
        "filename": "report.html",
        "expand": true,
        "openReport": false,
        "pageTitle": "Test Report - AVD API TESTS"
      }
    ]
  ]
}
